{
  "name": "vue-stack-tabs",
  "version": "1.0.0",
  "packageManager": "pnpm@10.29.3",
  "private": false,
  "type": "module",
  "files": [
    "src/lib",
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/vue-stack-tabs.es.js",
      "require": "./dist/vue-stack-tabs.umd.js"
    },
    "./nuxt": {
      "types": "./src/lib/nuxt/module.d.ts",
      "default": "./src/lib/nuxt/module.ts"
    },
    "./dist/style.css": "./dist/style.css",
    "./dist/vue-stack-tabs.css": "./dist/vue-stack-tabs.css"
  },
  "peerDependencies": {
    "nuxt": "^3.0.0 || ^4.0.0"
  },
  "peerDependenciesMeta": {
    "nuxt": {
      "optional": true
    }
  },
  "main": "dist/vue-stack-tabs.umd.js",
  "module": "dist/vue-stack-tabs.es.js",
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jktantan/vue-stack-tabs.git"
  },
  "keywords": [
    "vue",
    "vue router",
    "tabs",
    "nuxt"
  ],
  "author": "tantan",
  "license": "Apache License, Version 2.0",
  "bugs": {
    "url": "https://github.com/jktantan/vue-stack-tabs/issues"
  },
  "homepage": "https://github.com/jktantan/vue-stack-tabs.git",
  "scripts": {
    "clean:ports": "node -e \"const { execSync } = require('child_process'); try { if (process.platform === 'win32') { execSync('netstat -ano | findstr \":5173\" | findstr \"LISTENING\"').toString().split('\\n').forEach(line => { const pid = line.trim().split(/\\s+/)[4]; if (pid) try { execSync(`taskkill /F /PID ${pid}`, { stdio: 'ignore' }); } catch(e) {} }); execSync('netstat -ano | findstr \":5174\" | findstr \"LISTENING\"').toString().split('\\n').forEach(line => { const pid = line.trim().split(/\\s+/)[4]; if (pid) try { execSync(`taskkill /F /PID ${pid}`, { stdio: 'ignore' }); } catch(e) {} }); } else { execSync('lsof -ti:5173 | xargs kill -9 || true', { shell: '/bin/bash' }); execSync('lsof -ti:5174 | xargs kill -9 || true', { shell: '/bin/bash' }); } console.log('✅ 端口清理完成'); } catch(e) { console.log('✅ 端口已经空闲或清理完成'); }\"",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs && vite build --config vite.config.docs-demo.ts && node -e \"const fs=require('fs');const p='docs/.vitepress/dist/demo';fs.mkdirSync(p+'/demo',{recursive:true});fs.copyFileSync(p+'/index.html',p+'/demo/index.html');\"",
    "dev": "vite  --host ",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lib:build": "vite --config vite.config.lib.ts build && node -e \"require('fs').copyFileSync('dist/vue-stack-tabs.css','dist/style.css')\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:prepack": "node scripts/verify-prepack.mjs",
    "test:packaged": "pnpm run lib:build && node scripts/verify-packaged.mjs"
  },
  "dependencies": {
    "@juggle/resize-observer": "^3.4.0",
    "@mdit/plugin-tasklist": "^0.22.3",
    "@noble/hashes": "^2.0.1",
    "dayjs": "^1.11.19",
    "defu": "^6.1.4",
    "hex-encoding": "^2.0.3",
    "js-base64": "^3.7.8",
    "lz-string": "^1.5.0",
    "mitt": "^3.0.1",
    "normalize.css": "^8.0.1",
    "throttle-debounce": "^5.0.2",
    "uint8-encoding": "^2.0.1",
    "ulid": "^3.0.2",
    "vitepress": "^1.6.4",
    "vue": "^3.5.28",
    "vue-i18n-lite": "^1.0.2",
    "vue-router": "^5.0.2"
  },
  "devDependencies": {
    "@element-plus/icons-vue": "^2.3.2",
    "@eslint/js": "^10.0.1",
    "@rushstack/eslint-patch": "^1.15.0",
    "@tsconfig/node20": "^20.1.9",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^25.2.3",
    "@types/throttle-debounce": "^5.0.2",
    "@vitejs/plugin-vue": "^6.0.4",
    "@vitejs/plugin-vue-jsx": "^5.1.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.7.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "@vueuse/core": "^14.2.1",
    "element-plus": "^2.13.2",
    "eslint": "^10.0.0",
    "eslint-plugin-vue": "^10.8.0",
    "globals": "^17.3.0",
    "happy-dom": "^20.6.1",
    "lodash-unified": "^1.0.3",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.8.1",
    "sass": "^1.97.3",
    "sass-loader": "^16.0.7",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.56.0",
    "unplugin-auto-import": "^21.0.0",
    "unplugin-vue-components": "^31.0.0",
    "vite": "^7.3.1",
    "vite-plugin-dts": "4.5.4",
    "vite-plugin-package-version": "^1.1.0",
    "vitest": "^4.0.18",
    "vue-tsc": "^3.2.4"
  }
}
